{
  "name": "cipc-agent-army",
  "version": "1.0.0",
  "description": "AI-Powered CIPC Compliance Automation for South African SMMEs",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "cd server && tsx --env-file .env src/app.ts",
    "dev:client": "cd client && npm run dev",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cd client && npm run build",
    "build:server": "cd server && tsc",
    "start": "cd server && node dist/index.js",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest",
    "generate-hash": "node scripts/generate-hash.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.450.0",
    "@google/genai": "^1.17.0",
    "@google/generative-ai": "^0.24.1",
    "@langchain/openai": "^0.1.0",
    "@neondatabase/serverless": "^1.0.1",
    "@temporalio/client": "^1.13.0",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.44.5",
    "drizzle-zod": "^0.5.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "express-session": "^1.18.2",
    "helmet": "^8.1.0",
    "langchain": "^0.1.0",
    "langsmith": "^0.1.0",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^5.1.5",
    "openai": "^5.19.1",
    "pg": "^8.16.3",
    "pino": "^9.9.0",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.1.1",
    "postgres": "^3.4.7",
    "tailwindcss-animate": "^1.0.7",
    "vite": "^5.3.5",
    "ws": "^8.18.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@replit/vite-plugin-runtime-error-modal": "^0.0.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.2",
    "@types/jest": "^30.0.0",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.12",
    "@types/pg": "^8.15.5",
    "@types/pino-http": "^5.8.4",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "drizzle-kit": "^0.31.4",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "postcss": "^8.4.39",
    "tailwindcss": "^3.4.10",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "keywords": [
    "cipc",
    "compliance",
    "ai",
