# Environment variables for CIPC-Agent

# Database
DATABASE_URL="postgresql://user:password@localhost:5432/cipc_agent"

# WhatsApp Integration (AiSensy - to be replaced by Typebot)
AISENSY_API_KEY="your_aisensy_api_key"
AISENSY_BASE_URL="https://api.aisensy.com/v1"

# Temporal
TEMPORAL_GRPC_ENDPOINT="localhost:7233"
TEMPORAL_NAMESPACE="default"

# Security
JWT_SECRET="your_jwt_secret"
API_KEY="your_api_key"

# Vercel
VERCEL_PROJECT_ID="your_vercel_project_id"
VERCEL_ORG_ID="your_vercel_org_id"

# Typebot Configuration
TYPEBOT_BUILDER_URL=http://localhost:3001
TYPEBOT_VIEWER_URL=http://localhost:3002
TYPEBOT_ENCRYPTION_SECRET=your-secret-here
NEXTAUTH_SECRET=your-nextauth-secret
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret

# WhatsApp Business API (for Typebot integration)
WHATSAPP_BUSINESS_ID=your-whatsapp-business-id
WHATSAPP_ACCESS_TOKEN=your-whatsapp-access-token
WHATSAPP_PHONE_NUMBER_ID=your-whatsapp-phone-number-id
WEBHOOK_VERIFY_TOKEN=your-webhook-verify-token

# AI Orchestrator
GEMINI_API_KEY=your-gemini-api-key
CIPC_API_ENDPOINT=your-cipc-api-endpoint
POPIA_COMPLIANCE_MODE=true
AUDIT_LOG_LEVEL=detailed

# Analytics Engine
CLICKHOUSE_URL=http://localhost:8123
REDIS_URL=redis://localhost:6379
METRICS_RETENTION_DAYS=90
REAL_TIME_ALERTS=true

# PostgreSQL for Typebot and Analytics
POSTGRES_PASSWORD=your-postgres-password